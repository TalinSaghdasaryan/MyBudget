#include <iostream>
using namespace std;

class Income{
 private:
  double amount;
  string type;
 public:
  void getIncome(){
    cin>>amount;
    cin>>type;
  }
  double getAmount(){
      return amount;
  }
};

class Outcome{
 private:
  double amount=0.0;
  string type;
  double current=0.0;
  double percentage;
  double budget;
 public:
  void getOutcome(double realincome){
    cin>>type;
    cin>>percentage;
    budget = realincome*percentage/100;
  }
  string getType(){
      return type;
  }
  void coutType(){
      cout<<type;
  }
  double getAmount(){
      return amount;
  }
  void coutAmount(){
      cout<<amount;
  }
  double getBudget(){
      return budget;
  }
  void getOutcomeAmount(){
    cin>>current;
    amount = amount+current;
    budget = budget - current;
    if (budget <= 0){
        cout<<"Error 500"<<endl;
        /*cout<<"Would you like to:"<<endl<<"1. take this out of another income or "<<endl<<"2. do we not take this BS into account?"<<endl;
        cout<<"If 1, press 1, if 2 press 2";
        int a;
        cin>>a;
        if(a == 1){
            
        }
        if (a==2){
            
        }
        else{
            cout<<"Your input is incorrect"<<endl;
        }*/
    }
  }
};

    void bs(Outcome *outcome, int n);

int main(){
    cout<<"jjj"<<endl;
    double realincome=0.0;
    int m;
    cout<<"jjj"<<endl;
    cin>>m;
    Income * income;
    income = new Income [m];
    for(int i=0; i<m; i++){
        income[i].getIncome();
        realincome = realincome + income[i].getAmount();
    }
    int n;
    cin>>n; 
    Outcome *outcome;
    outcome = new Outcome [n];
    for(int i=0; i<n; i++){
        outcome[i].getType();
    }
    int a=1;
    string type;
    while(a == 1){
        for(int i=0; i<n; i++){
            if(type == outcome[i].getType()){
                outcome[i].getOutcomeAmount();
            }
        }
        cin>>a;
    }
    bs(*outcome, n);
    
    return 0;
}

    void bs(Outcome *outcome, int n){
        int ooverall = 0;
        int ioverall = 0;
        for (int i=0; i<n; i++){
            ooverall = ooverall + outcome[i].getAmount();
        }
        for (int i=0; i<n; i++){
            ioverall = ioverall + outcome[i].getBudget();
        }
        for(int i=0; i<n; i++){
            cout<<"On "<<outcome[i].coutType()<<" you have spent "<<outcome[i].coutAmount()<<endl;
        }
        cout<<"Overall you have spent "<<ooverall<<endl;
        cout<<"You have "<<ioverall<<" left"<<endl;
    }
