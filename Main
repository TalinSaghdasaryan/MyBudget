#include <iostream>
using namespace std;

class Income{
public:
  int in_amount;
  string type;
  void getIncome(){
    cin>>in_amount;
    cin>>type;
  }
  int amount;
  
};

class Outcome{
public:
  int amount=0;
  string type;
  void getOutcome(){
    cin>>amount;
    cin>>type;
  }
};

class Budget{
  int percentage;
  string tincome;
  string toutcome;
  void getBudget(){
    cin>>percentage;
    cin>>tincome;
    cin>>toutcome;
  }
};

int main(){
  Income income[3];
  for(int i=0; i<3; i++){
    income[i].getIncome();
  }
  Outcome outcome[5];
  for(int i=0; i<5; i++){
    outcome[i].getOutcome();
  }
  return 0;
}
